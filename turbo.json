{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".svelte-kit/**", ".astro/**", "src-tauri/target/release/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"cleanup": {},
		"test": {
			"cache": false
		}
	}
}
